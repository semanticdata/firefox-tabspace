{
  "manifest_version": 3,
  "name": "TabSpace",
  "description": "A scratch space for your new tab page.",
  "version": "1.3.1",
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@tabspace.com",
      "strict_min_version": "100.0"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}