{
  "manifest_version": 3,
  "name": "TabSpace",
  "description": "A scratch space for your new tab page.",
  "version": "1.4.0",
  "browser_specific_settings": {
    "gecko": {
      "id": "addon@tabspace.com",
      "strict_min_version": "100.0"
    }
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "128": "logo128.png",
    "192": "logo192.png",
    "512": "logo512.png"
  }
}
